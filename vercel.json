{
  "cleanUrls": true,
  "trailingSlash": false,
  "outputDirectory": ".",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://sugar1.com/$1",
      "statusCode": 301,
      "has": [
        {
          "type": "host",
          "value": "www.sugar1.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://sugar1.com/$1",
      "statusCode": 301,
      "has": [
        {
          "type": "host",
          "value": "sugar1com.vercel.app"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}